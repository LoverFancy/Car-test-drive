{
  "no-console": [
    true,
    "log",
    "dir",
    "table"
  ],
  "space-before-function-paren": [
    "error",
    {
      "anonymous": "always",
      "named": "always",
      "asyncArrow": "always"
    }
  ],
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-empty": false
  }
}